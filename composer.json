{
    "name": "internetgalerie/ig-slug",
    "type": "typo3-cms-extension",
    "description": "Rebuild URL slugs",
    "keywords": [
	"TYPO3",
	"extension",
	"slug",
	"url"
    ],
    "homepage": "https://www.internetgalerie.ch",
    "authors": [
	{
	    "name": "Daniel Abplanalp",
	    "role": "Developer",
	    "homepage": "https://www.internetgalerie.ch"
	}
    ],
    "license": ["GPL-2.0-or-later"],
    "require": {
      "typo3/cms-core": "^9.5.0 || ^10.4 || ^11"
    },
    "autoload": {
        "psr-4": {
            "Ig\\IgSlug\\": "Classes"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Ig\\IgSlug\\Tests\\": "Tests"
        }
    },
    "replace": {
        "typo3-ter/ig-slug": "self.version"
    },
    "extra": {
	"typo3/cms": {
	    "extension-key": "ig_slug"
	}
    }
}